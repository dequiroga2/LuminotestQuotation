{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": null,
  "ignoreCommand": "exit 0",
  "functions": {
    "api/index.ts": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ],
  "outputDirectory": "dist/public"
}
